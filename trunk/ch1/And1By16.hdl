/**
 * 1 by 16-bit and. Out[i] = sel & a[i]
 */

CHIP And1By16 {

    IN  a[16], sel;
    OUT out[16];

    PARTS:
    And(a=sel, b=a[0], out=out[0]);
    And(a=sel, b=a[1], out=out[1]);
    And(a=sel, b=a[2], out=out[2]);
    And(a=sel, b=a[3], out=out[3]);
    And(a=sel, b=a[4], out=out[4]);
    And(a=sel, b=a[5], out=out[5]);
    And(a=sel, b=a[6], out=out[6]);
    And(a=sel, b=a[7], out=out[7]);
    And(a=sel, b=a[8], out=out[8]);
    And(a=sel, b=a[9], out=out[9]);
    And(a=sel, b=a[10], out=out[10]);
    And(a=sel, b=a[11], out=out[11]);
    And(a=sel, b=a[12], out=out[12]);
    And(a=sel, b=a[13], out=out[13]);
    And(a=sel, b=a[14], out=out[14]);
    And(a=sel, b=a[15], out=out[15]);
}
